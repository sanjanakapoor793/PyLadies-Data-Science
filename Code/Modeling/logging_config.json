{
    "version" : 1,
    
    "disable_existing_loggers" :  "False",
    
    "formatters" : {
        "standard" : {
            "format" : "%(levelname)s  %(name)s  %(asctime)s %(filename)s:%(lineno)s] %(message)s"
        }
    },

    "handlers" : {
        "default" : {
            "level" : "DEBUG",
            "formatter" : "standard",
            "class" : "logging.StreamHandler"
        }
    },

    "loggers" : {
        "" : {
            "handlers" : ["default"],
            "level" : "INFO",
            "propogate" : "True"
        }
    }
}
